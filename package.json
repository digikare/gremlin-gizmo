{
  "name": "gremlin-gizmo",
  "version": "0.0.1",
  "description": "Gremlin-gizmo is a Gremlin object modeling tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digikare/gremlin-gizmo.git"
  },
  "keywords": [
    "gremlin",
    "nodejs",
    "graph",
    "janusgraph",
    "graphdb",
    "orm",
    "db"
  ],
  "author": "Tieu-Philippe KHIM <pkhim@digikare.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digikare/gremlin-gizmo/issues"
  },
  "homepage": "https://github.com/digikare/gremlin-gizmo#readme",
  "dependencies": {
    "gremlin": "^3.3.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6"
  }
}
